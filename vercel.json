{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "dist/public",
	"framework": null,
	"functions": {
		"api/**/*.ts": {
			"runtime": "nodejs18.x"
		}
	},
	"rewrites": [
		{ "source": "/(.*)", "destination": "/index.html" }
	]
}
